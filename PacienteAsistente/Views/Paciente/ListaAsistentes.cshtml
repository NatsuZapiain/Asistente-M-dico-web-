@model PacienteAsistente.Model.Models.ListaAsistentesViewModel
@{
    ViewBag.Title = "Lista Asistentes";
    Layout = "../Shared/_MyLayout.cshtml";
}

<h2>Lista Asistentes</h2>

<div>
    <table class="table">
        <thead>
        <tr>
            <th>Nombre Completo</th>
            <th>Parentesco</th>
            <th>Telefono</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model.ListaAsistentes)
        {
            <tr>
                <td>@item.Nombre</td>
                <td>@item.Parentesco</td>
                <td>@item.Telefono</td>
            </tr>
        }
        </tbody>
    </table>
</div>